package br.com.unitri.posjava.java8.interfacesfuncionais;

public class Main {
	
	public static void main(String[] args) {
		
		Regra regra1 = new MinhaRegra();
		
		Regra regra2 = new Regra() {

			@Override
			public boolean valida(Object usuario) {
				
				if(usuario.getPontos()>10)
					return true;
				
				return false;
			}
			
		};
		
		
		
	}

}
