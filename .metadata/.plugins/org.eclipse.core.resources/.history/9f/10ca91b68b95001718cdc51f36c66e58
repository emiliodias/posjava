package br.com.unitri.posjava.colecoes.maps;

public class Cliente {
	
	private String nome;
	
	private String id;
	
	private String tipo;

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

}
